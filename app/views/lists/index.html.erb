<h1>Lists#index</h1>
<p>Find me in app/views/lists/index.html.erb</p>
<ul>
  <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list) %>
    <%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill %></li>
  <% end %>
</ul>
<%= link_to "+ add new list", new_list_path %>
